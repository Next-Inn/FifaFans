{
  "name": "fifafans",
  "version": "0.0.0",
  "description": "FifaFan is a platform that connect football players, fans, personnel",
  "private": true,
  "engines": {
    "node": "12.18.3"
  },
  "scripts": {
    "lint": "eslint --fix \"./src/**/*.js\"",
    "test": "cross-env NODE_ENV=test npm run db:migrate:undo && npm run db:migrate && nyc mocha --no-timeout './tests/index.js' --exit",
    "test:report": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text-summary mocha --timeout 5000 './tests/index.js' --exit",
    "dev": "nodemon --exec babel-node src/index.js",
    "start": "npm run build && node build/index.js",
    "build": "babel src --out-dir build",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "db:migrate:test": "./node_modules/.bin/sequelize db:migrate --env test",
    "db:seed:all:test": "./node_modules/.bin/sequelize db:seed:all --env test",
    "db:migrate:undo": "node_modules/.bin/sequelize db:migrate:undo:all",
    "db:seed:all": "./node_modules/.bin/sequelize db:seed:all",
    "db:generate:model": "npx sequelize model:generate",
    "db:seed:undo": "npx sequelize db:seed:undo",
    "coverage": "cross-env NODE_ENV=test nyc npm run test && nyc report --reporter=lcov --reporter=text-lcov --exit",
    "test-a": "cross-env NODE_ENV=test nyc npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Next-Inn/FifaFans.git"
  },
  "author": "Tueloper, Lovisgod, Promise, Ogochukwu",
  "license": "ISC",
  "homepage": "https://github.com/Next-Inn/FifaFans#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "canvas": "^2.6.1",
    "cloudinary": "^1.23.0",
    "cookie-parser": "^1.4.5",
    "cross-env": "^5.2.0",
    "datauri": "^2.0.0",
    "debug": "~2.6.9",
    "dotenv": "^8.1.0",
    "express": "~4.16.1",
    "express-handlebars": "^3.1.0",
    "google-auth-library": "^6.0.6",
    "hbs": "^4.1.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "lightbox2": "^2.11.3",
    "morgan": "~1.9.1",
    "multer": "^1.4.1",
    "newsapi": "^2.4.1",
    "nodemailer": "^6.4.11",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "path": "^0.12.7",
    "pg": "^7.12.1",
    "redis": "2.8.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.6",
    "sequelize": "^5.22.3",
    "sequelize-cli": "^5.5.0",
    "socket.io": "^2.3.0",
    "uuid": "^3.3.3",
    "@babel/cli": "^7.10.5",
    "@babel/compat-data": "^7.11.0",
    "@babel/core": "^7.11.4",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.10.5",
    "babel-eslint": "^10.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/compat-data": "^7.11.0",
    "@babel/core": "^7.11.4",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.10.5",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.1.0",
    "eslint": "^6.2.2",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "faker": "^4.1.0",
    "mocha": "^7.2.0",
    "nodemailer-mock": "^1.5.3",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0"
  },
  "nyc": {
    "exclude": [
      "src/models/index.js",
      "src/models",
      "tests",
      "node_modules"
    ],
    "require": [
      "@babel/register"
    ],
    "sourceMap": false,
    "cache": false,
    "report-dir": "tests/report"
  }
}
