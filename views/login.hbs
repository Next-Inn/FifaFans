
<div class="central sign-up-container">
    <div class="card m-auto mt-5 width-50">
         <div class="card-body">
             <div class="text-center">
                {{!-- <img src="" title="logo"> --}}
                <h3 class="mt-3 mb-2 color-blue"><a href="/"><b>Fifafan</b></a></h3>
                <p class="text-center">Don't have an account? <a class="login-a ml-2" href="/signup">Sign up</a></p>
             </div>
            <form id="loginForm" action="">
                 <p id="error-message"></p>
                <p id="success-message"></p>
                 <div class="form-group">
                    <input type="email" class="form-control" placeholder="Email address" id="email" aria-describedby="emailHelp">
                    
                </div>
                <div class="form-group">
                    <input type="password" placeholder="Password" class="form-control" id="password">
                </div>
                <button id="login-button" type="submit" class="btn btn-primary width-100">Submit</button>
                <div class="text-center">
                    <p class="text-center my-3">Forgot Password?<a  class="login-a ml-2" href="/forgetpassword">Reset Here</a></p>
                </div>
            </form>
        </div>
        
    </div>
</div>
